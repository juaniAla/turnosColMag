<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}TurnosWeb!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('css/app') }}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            {% block body %}{% endblock %}            
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('js/app') }}
            <script src="{{ asset('js/locales/bootstrap-datepicker.es.min.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
